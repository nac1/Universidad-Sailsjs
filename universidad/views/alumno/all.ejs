<table  class="striped bordered highlight responsive-table">
        <thead>
          <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Last name</th>
              <th>Age</th>
              <th>Career</th>
              <th colspan="2" class="center">Options</th>

          </tr>
        </thead>

        <tbody>
        <% _.each(alumnos,function(alumno){%>
          <tr>
            <td><%= alumno.matricula%></td>
            <td><%= alumno.nombre%></td>
            <td><%= alumno.apellido_paterno%> <%= alumno.apellido_materno%></td>
            <td><%= alumno.edad%></td>
            <td><%= alumno.cursa%></td>
            <td>
              <form class="col s12" method="POST" action="/alumno/show/?matricula=<%=alumno.matricula%>">
              <button class="btn waves-effect waves-light #00b0ff light-blue accent-3" type="submit" name="action">Show
              <i class="material-icons right">mode_edit</i>
              </button>
              </form>
            </td>

            <td>
              <form class="col s12" method="POST" action="/alumno/delete/?matricula=<%=alumno.matricula%>">
              <button class="btn waves-effect waves-light #c62828 red darken-3" type="submit" name="action">Delete
              <i class="material-icons right">delete</i>
              </button>
              </form>
            </td>
          </tr>
        <%  }) %>
        </tbody>
      </table>